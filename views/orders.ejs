<body>
  <!-- Header -->
  <%- include('./partials/header'); -%>

    <!-- Body -->
        <div class="subforum">
          <a href = "/orders/create">
            <button type="button" class="btn btn-cyan btn-sm">Add Order</button>
          </a>
          <!--Pending Oders_____________ -->
          <div class="subforum-title">
            <h1>Pending Orders</h1>
          </div>
          <% if (orP.length> 0) { %>
            <% orP.forEach(orP=> { %>
              <div class="subforum-row">
                <div class="subforum-icon subforum-column center">
                  <img src="images/Pen.png" width="90px" height="90px"/>
                  <i class="fa fa-car center"></i>
                </div>
                <div class="subforum-description subforum-column">
                  <h4><a href="/orders/<%= orP._id %>">Num # <%= orP.orderNum%></a></h4>
                  <h1>بيض ابيض: <%= orP.eggW%></>
                  <h1>بيض احمر: <%= orP.eggR%></h1>
                  <h1>بيض بلدي: <%= orP.eggB%></h1>
                </div>
                <div class="subforum-stats subforum-column center">
                  <span>24 Posts | 12 Topics</span>
                </div>
                <div class="subforum-info subforum-column">
                  <b><a href="">Last post</a></b> by <a href=""><%=orP.user  %></a>
                  <br>on <small>Date: <%= orP.published %></small>
                </div>
              </div>

              <% }) %>
                <% } else { %>
                  <h1>There are no Reports to display...</h1>
                  <% } %>
                    <!--Ready Oders_____________ -->
